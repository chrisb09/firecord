@startuml DataType

abstract AbstractData

interface SimpleInterface<T> {
    + T get()
    + boolean set()
    + boolean setIfEmpty()
}

interface ByteDataInterface {
    + byte[] getData();
    + Bytes getBytes();
}

class Bytes implements ByteDataInterface {
    + Bytes append()
    + void set()
}

enum JedisCommunicationChannel implements ByteDataInterface {
    + int getId()
}


abstract AbstractData <T> {
    + {static} HashMap<Bytes, AbstractData> loaded
    + {static} AbstractData create()
    + AbstractData lock()
    + boolean tryLockMultiple()
    + void close()
    + void unlockMultiple()
    + Bytes getKey()
    + String asString()
}

abstract SmallData <T> extends SimpleData {

}

abstract LargeData <T> extends SimpleData {

}

abstract SimpleData<T> extends AbstractData implements SimpleInterface {
    + {static} HashMap<Bytes, SimpleData> loaded
}

class Invalid extends AbstractData {

}

abstract NumericData<T> extends SmallData {
    + {abstract} T add();
    + {abstract} T sub();
    + {abstract} T mul();
    + {abstract} T div();
}

class RBoolean extends SmallData {
    
}

class RCharacter extends SmallData {
    
}

class RString extends SmallData {
    
}

class RVector extends SmallData {
    
}

class RWrapper<T> extends SmallData {
    + void setIfEmpty()
    + T getDefaultValue()
}

class RByte extends NumericData {
    
}

class RShort extends NumericData {
    
}

class RInteger extends NumericData {

}

class RLong extends NumericData {
    
}

class RFloat extends NumericData {
    
}

class RDouble extends NumericData {

}


class RItemStack extends LargeData {
    
}

class RByteArray extends LargeData {
    
}

class RJson extends LargeData {
    + boolean hasKey()
    + String getAsString()
    + int getAsInt()
    + long getAsLong()
    + double getAsDouble()
    + boolean getAsBoolean()
    + Enum getAsEnum()
    + Number getAsNumber()
    + void putString()
    + void putInt()
    + void putLong()
    + void putDouble()
    + void putBoolean()
    + void putEnum()
}


abstract CompositeData <T extends AbstractData> extends AbstractData {
    + {static} HashMap<Bytes, CompositeData<?,?>> loaded
    + void clear()
    + boolean isEmpty()
    + int size()
}

abstract CollectionData <T extends AbstractData,\nE extends Collection<T>> extends CompositeData implements Collection {
    + void set()
    + boolean removeKey()
    + boolean contains()
    + boolean containsKey()
    + boolean containsValue()
    + boolean containsAll()
    + Iterator<T> iterator()
    + Object[] toArray()
    + F[] toArray()
}

class RList <T extends AbstractData> extends CollectionData {
    + boolean add()
    + void add()
    + boolean addAll()
    + T get()
    + int indexOf()
    + int lastIndexOf()
    + ListIterator<T> listIterator()
    + boolean remove()
    + List<Integer> indicesOf()
    + T remove()
    + boolean removeAll()
    + boolean retainAll()
    + T set()
    + List<T> subList()

}

CompositeData o-- AbstractData

abstract AbstractObject extends AbstractData {
    # RByte RByte()
    # RShort RShort()
    # RInteger RInteger()
    # RLong RLong()
    # RFloat RFloat()
    # RDouble RDouble()
    # RBoolean RBoolean()
    # RByteArray RByteArray()
    # RCharacter RCharacter()
    # RRItemStack() RItemStack()
    # RJson RJson()
    # RString RString()
    # RUUID RUUID()
    # RWrapper RWrapper()
    # REnum REnum()
}

AbstractObject *-- AbstractData

abstract SimpleAbstractObject extends AbstractObject implements SimpleInterface {

}

class REnum extends SimpleAbstractObject {

}

REnum *-- RString


class DataGenerator<T extends AbstractData> {
    + {static} AbstractData<?> createAnonymous()
    + {static} void delete()
    + T create()
}

