abstract AbstractData <T> {
    + {static} AbstractData create()
    + AbstractData lock()
    + void close()
    + String getKey()
}

abstract SmallData <T> extends SimpleData {

}

abstract LargeData <T> extends SimpleData {

}

abstract SimpleData<T> extends AbstractData {
    + T get()
    + void set()
}

abstract NumericData<T> extends SmallData {
    + {abstract} T add();
    + {abstract} T sub();
    + {abstract} T mul();
    + {abstract} T div();
}

class RBoolean extends SmallData {
    
}

class RString extends SmallData {
    
}

class RVector extends SmallData {
    
}

class RInteger extends NumericData {

}

class RDouble extends NumericData {

}


class RItemStack extends LargeData {
    
}

