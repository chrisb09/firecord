}


abstract CompositeData <T extends AbstractData,E> extends AbstractData {
    + void set()
    + boolean removeKey()
    + void clear()
    + boolean contains()
    + boolean containsAll()
    + boolean isEmpty()
    + Iterator<T> iterator()
    + int size()
    + Object[] toArray()
    + F[] toArray()
}

class RList <T extends AbstractData> extends CompositeData {
    + boolean add()
    + void add()
    + boolean addAll()
    + T get()
    + int indexOf()
    + int lastIndexOf()
    + ListIterator<T> listIterator()
    + boolean remove()
    + List<Integer> indicesOf()
    + T remove()
    + boolean removeAll()
    + boolean retainAll()
    + T set()
    + List<T> subList()

}

